{
  "name": "versade",
  "version": "1.0.0",
  "description": "Find package versions and documentation URLs with strategic precision",
  "author": "codewithkenzo",
  "license": "MIT",
  "server": {
    "command": "python",
    "args": ["-m", "versade"],
    "env": {
      "VERSADE_PORT": "9373",
      "VERSADE_LOG_LEVEL": "INFO"
    }
  },
  "schemas": {
    "config": {
      "type": "object",
      "properties": {
        "port": {
          "type": "number",
          "default": 9373,
          "description": "Port for the Versade server"
        },
        "log_level": {
          "type": "string",
          "enum": ["DEBUG", "INFO", "WARNING", "ERROR", "CRITICAL"],
          "default": "INFO",
          "description": "Logging level"
        }
      }
    }
  }
}
